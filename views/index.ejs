<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>
<body>




    <%- include('./partials/nav.ejs') %>

    <div class="blogs content">
        <h2>All blogs</h2>
    </div>

    <% if(blogs.length > 0) { %>
        <% blogs.forEach(blog =>{ %>
        <h3> <%=blog.title %> </h3>
        <p> <%=blog.snippet %> </p>
       <% }) %>
    <% }else { %>
        <p>There are no blogs to display </p>
   <% } %>

   <%- include('./partials/footer.ejs') %>
</body>
</html>